<h1>New parent</h1>

<% form_for(@parent) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :parent_chain_id %><br />
    <%= f.text_field :parent_chain_id %>
  </p>
  <p>
    <%= f.label :child_chain_id %><br />
    <%= f.text_field :child_chain_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', parents_path %>