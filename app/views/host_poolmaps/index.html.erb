<h1>Listing host_poolmaps</h1>

<table>
  <tr>
    <th>Pool</th>
    <th>Host</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @host_poolmaps.each do |host_poolmap| %>
  <tr>
    <td><%= host_poolmap.pool_id %></td>
    <td><%= host_poolmap.host_id %></td>
    <td><%= link_to 'Show', host_poolmap %></td>
